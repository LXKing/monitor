package com.edata.godp.domain.firmware;

public class DownloadPageDataResponse {
    public byte[] content;
    private short index;

    /**
     * 获取页序号
     *
     * @return 页序号
     */
    public short getIndex() {
        return index;
    }

    public void setIndex(short index) {
        this.index = index;
    }

    /**
     * 获取内容
     *
     * @return 内容
     */
    public byte[] getContent() {
        return content;
    }

    /**
     * 设置内容
     *
     * @param content 内容
     */
    public void setContent(byte[] content) {
        this.content = content;
    }
}
