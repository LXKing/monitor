.box7_drag{
  width: 0.1%;
  height: 100%;
  background-color: #fff;
  position: absolute;
  left: 20%;
  top: 0;
}
.box7_drag::before{
  content:'';
  background:url("./../image/zuoyou.png") no-repeat;
  background-size: contain;
  display: inline-block;
  width: 6px;
  height: 60px;
  position: absolute;
  left: -4px;
  top: 43%;
}
.driving_map{
  width: 79.9%;
  height: 100%;
  // background-color: orange;
  position: absolute;
  left: 20.1%;
  top: 0;
  .dm_top{
    height: 69.9%;
    color: #666;
  }
  .dm_drag{
    width: 100%;
    height: 0.5%;
    background-color: #fff;
    z-index: 1000;
  }
  .dm_drag::before{
    content:'';
    background:url("./../image/shangxia.png") no-repeat;
    background-size: contain;
    display: inline-block;
    width: 78px;
    height: 6px;
    position: absolute;
    left: 48%;
     //top: -3px;
  }
  .dm_mb{
    height: 29.6%;
    .dm_middle{
      //height: 132px;
      width: 100%;
      color: #000;
      overflow: scroll;
      //position: absolute;
      //left: 0;
      //bottom: 15px;
      .con_7{
        display: none;
        table{
          width: 100%;
          color: #666;
          text-align: center;
          thead{
            color: #fff;
            background-color: #0488d1;
          }
          // 1010101010101010
          tbody{
            tr:nth-child(even){
              background-color: #e5e3e4;
            }
            tr:hover{
              background-color: #ffe8e0;
            }
          }
        }
      }


    }
    .dm_bottom{
      width: 100%;
      height: 15px;
      line-height: 15px;
      background-color:#fff;
      position: absolute;
      left: 0;
      bottom: 0;
      //clear: both;
      // border-top: 1px solid #0469a1;
      .dm_span{
        width: 10%;
        float: left;
        text-align: center;
        color: #00ade1;
        background-color: #fff;
        border: 1px solid #0469a1;
        border-radius: 5px;
        box-sizing: border-box;
      }
      .cur_dmb{
        color:#fff;
        background-color: #0488d1;
      }
      .paging{
        float: right;
        height: 15px;
        width: 90%;
        // background: gray;
        display: inline-block;
        .layui-laypage {
          margin: 0;
        }
        .layui-laypage a, .layui-laypage span{
          margin: 0;
          height: 15px;
          line-height: 15px;
          padding: 0 5px;
        }
        .layui-laypage select{
          height: 15px;
          line-height: 15px;
          padding: 0;
          border: none;
        }
        .layui-laypage button, .layui-laypage input{
          height: 15px;
          line-height: 15px;
        }
        .layui-laypage .layui-laypage-curr .layui-laypage-em{
          background-color: #00ade1;
        }
      }
    }
  }

}