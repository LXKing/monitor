    .dl_bottom{
      width: 100%;
      height: 15px;
      // background-color: red;
      position: absolute;
      left: 0;
      bottom: 0;
      background-color: #fff;
      .layui-laypage {
        margin: 0;
      }
      .layui-laypage a, .layui-laypage span{
        margin: 0;
        height: 15px;
        line-height: 15px;
        padding: 0 1.5px;
      }
      .layui-laypage select{
        height: 15px;
        line-height: 15px;
        padding: 0;
        border: none;
      }
      .layui-laypage button, .layui-laypage input{
        height: 15px;
        line-height: 15px;
      }
      .layui-laypage .layui-laypage-curr .layui-laypage-em{
        background-color: #00ade1;
      }
      .layui-laypage-next em, .layui-laypage-prev em {
        /* font-family: Sim sun; */
        font-size: 12px;
      }
      .layui-laypage input{
        display: inline-block;
        width: 20px;
        margin: 0;
        padding: 0;
        text-align: center;
