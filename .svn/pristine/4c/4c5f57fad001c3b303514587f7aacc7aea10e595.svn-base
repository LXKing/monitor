.driving_list{
  width: 20%;
  position: absolute;
  left: 0;
  top: 0;
  // background-color: tomato;
  .dl_header{
    width: 100%;
    height: 24px;
    line-height: 24px;
    // background-color: brown;
    text-align: center;
    .dlh_b{
      color: #fff;
      background-color: #0488d1;
    }
    span{
      width: 100%;
      float: left;
      background-color: #fff;
    }
  }
  .dis_dl{
    display: none;
  }
  .dla_all{
    .dl_search_up{
      width: 100%;
      height: 25px;
      // background-color: yellow;
      #cm_card{
        width: 40%;
        height: 25px;
        line-height: 25px;
        background-color: #fff;
        color: #666;
        position: relative;
        input{
          width: 100%;
          height: 25px;
          border: 1px solid #b7b7b7;
          box-sizing: border-box;
          border-radius: 6px;
          color: #666;
        }
        #cm_cardwarper{
          background-color: #fff;
          #pop {
            border: 1px solid #0488d1;
            padding: 0px;
            position: absolute;
            background-color: #fff;
            z-index: 10;
            ul{
              list-style-type: none;
              margin: 5px;
              padding: 0px;
            }
          }
        }
      }
      .sl_day{
        width: 40%;
        height: 25px;
        // background-color: orange;
        .layui-form-select .layui-input{
          padding-right:0px;
          padding-left: 3px;
          height: 25px;
          color:#666;
          background-color: #fff;
        }

        .layui-form-select .layui-edge{
          right: 1px;
          border-width: 5px;
        }
        .layui-form-select dl{
          top: 25px;
        }
        .layui-anim{
          width: 70px;
          color: #000;
        }
      }
      .dl_cd{
        width: 20%;
        height: 25px;
        button{
          background-color: #0488d1;
          width: 100%;
          height: 25px;
        }
        button:hover{
          opacity: .8;
        }
      }
    }
    .dl_search_down{
      width: 100%;
      height: 25px;
      // background-color: purple;
      .start_date{
        width: 48%;
        height: 25px;
        background-color:green;
        .layui-input{
          height: 25px;
          color:#666;
        }
      }
      .end_date{
        width: 48%;
        height: 25px;
        background-color: pink;
        .layui-input{
          height: 25px;
          color:#666;
        }
      }
    }
    .dl_way{
      height: 20px;
      line-height: 20px;
      // background-color: skyblue;
      color: #000;
      padding-left: 3px;
    }
    .dl_select{
      width: 100%;
      height: 25px;
      line-height: 25px;
      color: #000;
      text-align: center;
      span{
        width: 15%;
        // float: left;
        border: 1px solid #b7b7b7;
        box-sizing: border-box;
        border-radius: 6px;
        margin-left: 3px;
        cursor: pointer;
      }
      span:hover{
        opacity: .6;
      }
      .dl_s{
        color:#fff;
        background-color: #0488d1;
      }
    }
    .overlay{
      position: fixed;
      left: 0%;
      top: 52%;
      display: none;
      z-index: 999;
    }
    .dl_info{
      height: 432px;
      width: 100%;
      // background-color: orange;
      overflow: scroll;
      .dli{
        display: none;
      }
      .driving_all{
        ul{
          width: 100%;
          li{
            width: 98%;
            margin: auto 1%;
            margin-top: 10px;
            height: 155px;
            background-color: #f1f1f1;
            border:1px solid  #d6d5d5;
            color: #666;
            border-radius: 5px;
            position: relative;
            .stTime{
              width: 100%;
              height: 20px;
              line-height: 20px;
              padding-left: 15px;
              position: relative;
            }
            .stTime::before{
              content: '';
              background: url(../../dc_file/image/qidong.png) no-repeat;
              background-size: contain;
              width: 13px;
              height: 13px;
              position: absolute;
              left: 2px;
              top: 2px;
            }
            .stAdd{
              //text-align: center;
            }
            .edTime{
              width: 100%;
              height: 20px;
              line-height: 20px;
              padding-left: 15px;
              position: relative;
            }
            .edTime::before{
              content: '';
              background: url(../../dc_file/image/zhongdian.png) no-repeat;
              background-size: contain;
              width: 13px;
              height: 13px;
              position: absolute;
              left: 2px;
              top: 2px;
            }
            .edAdd{
              //text-align: center;
            }
            .dataList{
              width: 100%;
              text-align: center;
              position: absolute;
              top: 108px;
              table{
                width: 100%;

              }
            }
            .drivingPro{
              width: 100%;
              height: 15px;
              line-height: 15px;
              margin-top: 10px;
              position: absolute;
              bottom: 20px;
              span{
                float: left;
              }
              span:nth-child(1){
                width: 10%;
                img{
                  background-size: contain;
                  width: 15px;
                  height: 15px;
                }
              }
              span:nth-child(2){
                width: 60%;
              }
              span:nth-child(3){
                width: 10%;
                img{
                  background-size: contain;
                  width: 15px;
                  height: 10px;
                }
              }
              span:nth-child(4){
                width: 10%;
                img{
                  background-size: contain;
                  width: 15px;
                  height: 10px;
                }
              }
              span:nth-child(5){
                width: 10%;
              }
            }
            .drivingTime{
              width: 100%;
              height: 15px;
              text-align: center;
              margin-top: 5px;
            }
          }
          li:hover{
            border:1px solid #bed5e2;
            opacity: .8;
            //box-sizing: border-box;
          }
        }
      }
