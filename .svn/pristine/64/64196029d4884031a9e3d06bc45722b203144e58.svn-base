        .box7_drag{
            width: 0.1%;
            background-color: #fff;
            position: absolute;
            left: 20%;
            top: 0;
        }
        .box7_drag::before{
            content:'';
            background:url("./../image/zuoyou.png") no-repeat;
            background-size: contain;
            display: inline-block;
            width: 6px;
            height: 50px;
            position: absolute;
            left: -3px;
            top: 43%;
        }
        .driving_map{
            width: 79.9%;
            // background-color: orange;
            position: absolute;
            left: 20.1%;
            top: 0;
            .dm_top{
                height: 418px;
                // background-color: blue;
                color: #666;
            }
            .dm_drag{
                height: 1px;
                // background-color: red;
            }
            .dm_drag::before{
                content:'';
                background:url("./../image/shangxia.png") no-repeat;
                background-size: contain;
                display: inline-block;
                width: 50px;
                height: 5px;
                position: absolute;
                left: 48%;
                // top: -3px;
            }
            .dm_middle{
                height: 132px;
                color: #000;
                // background-color: purple;
                overflow: scroll;
                .con_7{
                    display: none;
                    table{
                        width: 100%;
                        color: #666;
                        text-align: center;
                        thead{
                            color: #fff;
                            background-color: #0488d1;
                        }
                        // 1010101010101010
                        tbody{
                            tr:nth-child(even){
                                background-color: #e5e3e4;
                            }
                            tr:hover{
                                background-color: #ffe8e0;
                            }
                        }
                    }
                }


            }
            .dm_bottom{
                width: 100%;
                height: 15px;
                background-color:#fff;
                position: absolute;
                left: 0;
                bottom: 0;
                clear: both;
                // border-top: 1px solid #0469a1;
                .dm_span{
                    width: 10%;
                    float: left;
                    text-align: center;
                    color: #00ade1;
                    background-color: #fff;
                    border: 1px solid #0469a1;
                    border-radius: 5px;
                    box-sizing: border-box;
                }
                .cur_dmb{
                    color:#fff;
                    background-color: #0488d1;
                }
                .paging{
                    float: right;
                    height: 15px;
                    width: 90%;
                    // background: gray;
                    display: inline-block;
                    .layui-laypage {
                        margin: 0;
                    }
                    .layui-laypage a, .layui-laypage span{
                        margin: 0;
                        height: 15px;
                        line-height: 15px;
                        padding: 0 5px;
                    }
                    .layui-laypage select{
                        height: 15px;
                        line-height: 15px;
                        padding: 0;
                        border: none;
                    }
                    .layui-laypage button, .layui-laypage input{
                        height: 15px;
                        line-height: 15px;
                    }
                    .layui-laypage .layui-laypage-curr .layui-laypage-em{
                        background-color: #00ade1;
                    }
                }
            }
        }
    }
    /*行程管理 结束*/

