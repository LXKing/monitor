/*公共头部开始*/
header {
    width: 100%;
    min-width: 1199px;
    height: 80px;
    overflow: hidden;
    .left_logo {
        width: 20%;
        height: 100%;
        background-color: #0488d1;
        padding-top: 15px;
        img{
            max-width: 66%;
            margin-left: 50px;
            margin-top: 4px;
        }
    }
    .right_tools {
        width:80%;
        height: 100%;
        nav{
            width: 75%;
            height: 100%;
            background-color: #0488d1;
            float: left;
            #nav{
                display: flex;
                justify-content: space-around;
                // margin-top: 20px;
                li{
                    color: #fff;
                    width: 100%;
                    height: 80px;
                    line-height: 33px;
                    border-left: 1px solid #1598e1;
                    border-right: 1px solid #0472ae;
                    // padding: 0 5px;
                    // position: relative;
                    background-color: #0488d1;
                    box-sizing: border-box;
                    text-align: center;
                    img{
                        height: 25px;
                        display: inline-block;
                        padding-top: 18px;
                    }
                    span{
                        display: block;
                    }
                }
                li:nth-child(10)::after{
                    // content:'';
                    // background:url("./../image/labadian.png ") no-repeat;
                    // vertical-align:middle;
                    // background-size: contain;
                    // display: inline-block;
                    // width: 8px;
                    // height: 13px;
                    // position: absolute;
                    // left: 45px;
                    // top: 0;
                }
                li:nth-child(11){
                    img{
                        // padding-left: 9px;
                    }
                }
            }
        }
        .user-info {
            width: 25%;
            height: 100%;
            background-color: #0488d1;
            float: left;
            // border-left: 2px solid #000;
            // box-sizing: border-box;
            .weather_msg{
                width: 60%;
                height: 80px;
                .wea_left{
                    width: 40%;
                    height:100%;
                    text-align: right;
                    line-height: 80px;
                    font-size: 15px
                }
                #wea_right{
                    width: 60%;
                    height: 100%;
                    display: inline-block;
                    position: relative;
                    // text-align: center;
                    img{
                        position: absolute;
                        width: 30px;
                        left: 8px;
                        top: 13px;
                    }
                    .wea_info{
                        position: absolute;
                        left: 52px;
                        top: 24px;
                    }
                    .w_date{
                        position: absolute;
                        left: 5px;
                        top: 43px;
                    }
                }
            }
            .login_user{
                width: 40%;
                height: 40px;
                //   line-height: 40px;
                font-size: 12px;
                margin-top: 25px;
                white-space: nowrap;
                //66666666666666666666666666666
                a{
                    width: 25%;
                    float: right;
                    color: #fff;
                    text-decoration: none;
                    padding-right: 22%;
                }
                a:nth-child(1){
                    padding-left: 5%;
                }
                a:nth-child(2){
                }
                a:nth-child(3){
                    padding-top: 3%;
                    padding-left: 5%;
                }
                a:nth-child(4){
                    padding-top: 3%;
                }
            }
        }
    }
}
/*公共头部结束*/


/*选项卡*/
.link-select {
    width: 100%;
    min-width: 1199px;
    height: 25px;
    position: absolute;
    left: 0;
    top: 80px;
    //background-color: #00ade1;
    border-bottom: 1px solid #dcdcdc;
    box-sizing: border-box;
    #tab{
        li {
            height: 25px;
            line-height: 25px;
            display: block;
            float: right;
            text-align: center;
            padding: 0 16px;
            border: 1px solid #00ade1;
            box-sizing: border-box;
            background-color: #0488d1;
            color: #fff;
            position: relative;
            margin-left: 2px;
            border-radius: 3px;
            cursor:pointer;
            i{
                position: absolute;
                left: 71px;
                top: -6px;
            }
        }
        li:hover{
            //background-color: #d5dfe4;
            opacity: .8;
        }
        .currentTab {
            background-color: #f3f2f1;
            color:#0488d1;
            border: 1px solid #dcdcdc;
            box-sizing: border-box;
        }
    }
}


