    <script>

        $.getJSON("http://api.help.bj.cn/apis/ip?ip=61.170.135.3", function (result) {
            function showLocation(res) {
                console.log(res);
            }

            console.log(result.data.city);
            var url = "http://api.map.baidu.com/telematics/v3/weather?location=";
            url += result.data.city;
            url += "&output=json&ak=E4805d16520de693a3fe707cdc962045&callback=showLocation";
            $.getJSON(url, function (result) {
                showLocation(result)
                console.log(result)
            });
        });

        var formatQuery = function(data) {
            var arr = [];
            for (var i in data) {
                // encodeURI()：用于整个 URI 的编码
                // encodeURIComponent：转义分隔 URI 各部分的标点符号，如空格" "转义为"20%"
                arr.push(encodeURIComponent(i) + "=" + encodeURIComponent(data[i]));
            }
            return arr.join("&");
        }
        var jsonp = function(options) {
            // 全局函数的名称，避免命名冲突或者缓存？使用随机数代替，这个函数会被callback=random传给后端
            var random = ("jsonp_" + Math.random()).replace(".", "");

            // 创建script标签并加入到页面中，head或body都行
            var h = document.getElementsByTagName("head")[0];
            var s = document.createElement("script");
            // URL里请求的参数与值，包含了上面定义的回调函数
            var req = formatQuery(options.request) + "&callback=" + random;

            // 创建jsonp全局回调函数，当后台返回response时script会自动执行
            window[random] = function(json) {
                clearTimeout(s.timer);
                // 处理返回的json，以参数形式传进来
                options.success && options.success(json);
                // 释放
                window[random] = null;
                // 收到回调后移除script标签
                h.removeChild(s);
                console.log("jsonp ok");
            };

            // 发送请求
            s.src = options.url + "?" + req;
            h.appendChild(s);
            // 设置超时10s，如果执行了，则认为请求失败，因为超时了没有返回
            s.timer = setTimeout(function() {
                window[random] = null;
                h.removeChild(s);
                console.log("jsonp failed");
            }, 10000);
        };
        window.jsonp = jsonp;

        jsonp({
            url: "http://api.map.baidu.com/telematics/v3/weather",

            request: {
                location: "北京",
                    output: "json",
                    ak: "E4805d16520de693a3fe707cdc962045"

            },
            success: getMessage
        })

        function getMessage(jsonp){
            alert("message:"+jsonp);
        }


        $.ajax({
            url: "http://api.map.baidu.com/telematics/v3/weather?location=%E5%8C%97%E4%BA%AC&output=json&ak=E4805d16520de693a3fe707cdc962045",
            dataType: "jsonp",
            jsonp:"callback",
            jsonpCallback:"getMessage",
            success: function (obj) {

                console.log(obj);
            }
        });



