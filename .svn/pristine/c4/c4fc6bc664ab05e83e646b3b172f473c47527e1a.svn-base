/*公共头部开始*/
header {
  height: 80px;
  overflow: hidden;
}
header .left_logo {
  width: 20%;
  height: 100%;
  background-color: #0469a1;
  padding-top: 15px;
}
header .left_logo img {
  max-width: 80%;
}
header .right_tools {
  width: 80%;
  height: 100%;
}
header .right_tools nav {
  width: 55%;
  height: 100%;
  background-color: #0469a1;
  float: left;
}
header .right_tools nav #nav {
  display: flex;
  justify-content: space-around;
  margin-top: 20px;
}
header .right_tools nav #nav li {
  color: #fff;
}
header .right_tools nav #nav li img {
  display: block;
  height: 30px;
}
header .right_tools nav #nav li span {
  font-size: 12px;
}
header .right_tools nav #nav li:hover img {
  transform: translateY(-9px);
  transition: all 0.5s;
}
header .right_tools nav #nav li:nth-child(1) img {
  padding-left: 8px;
}
header .right_tools nav #nav li:nth-child(2) img {
  padding-left: 12px;
}
header .right_tools nav #nav li:nth-child(3) img {
  padding-left: 5px;
}
header .right_tools nav #nav li:nth-child(4) img {
  padding-left: 9px;
}
header .right_tools nav #nav li:nth-child(5) img {
  padding-left: 8px;
}
header .right_tools nav #nav li:nth-child(6) img {
  padding-left: 7px;
}
header .right_tools nav #nav li:nth-child(7) img {
  padding-left: 6px;
}
header .right_tools nav #nav li:nth-child(8) img {
  padding-left: 6px;
}
header .right_tools nav #nav li:nth-child(9) img {
  padding-left: 8px;
}
header .right_tools nav #nav li:nth-child(10) {
  position: relative;
}
header .right_tools nav #nav li:nth-child(10) img {
  padding-left: 8px;
}
header .right_tools nav #nav li:nth-child(10)::after {
  content: '';
  background: url("../image/labadian.png ") no-repeat;
  vertical-align: middle;
  background-size: contain;
  display: inline-block;
  width: 8px;
  height: 13px;
  position: absolute;
  left: 45px;
  top: 0;
}
header .right_tools nav #nav li:nth-child(11) img {
  padding-left: 9px;
}
header .right_tools .user-info {
  width: 45%;
  height: 100%;
  background-color: #0469a1;
  float: left;
}
header .right_tools .user-info .weather_msg {
  width: 70%;
  height: 80px;
}
header .right_tools .user-info .weather_msg .wea_left {
  width: 65%;
  height: 100%;
  text-align: right;
  line-height: 80px;
  font-size: 15px;
}
header .right_tools .user-info .weather_msg #wea_right {
  width: 35%;
  height: 100%;
  display: inline-block;
  position: relative;
}
header .right_tools .user-info .weather_msg #wea_right img {
  position: absolute;
  width: 40px;
  left: 3px;
}
header .right_tools .user-info .weather_msg #wea_right .wea_info {
  text-align: center;
  height: 40px;
  line-height: 40px;
}
header .right_tools .user-info .weather_msg #wea_right .w_date {
  height: 40px;
  line-height: 40px;
}
header .right_tools .user-info .login_user {
  width: 30%;
  height: 40px;
  line-height: 40px;
  font-size: 12px;
}
header .right_tools .user-info .login_user a {
  float: left;
  color: #fff;
  text-decoration: none;
}
header .right_tools .user-info .login_user a:nth-child(1) {
  padding: 0 13px 0px 18px;
}
header .right_tools .user-info .login_user a:nth-child(3) {
  padding: 0 16px 0px 5px;
}
/*公共头部结束*/
/*选项卡*/
.link-select {
  width: 100%;
  min-width: 996px;
  height: 25px;
  position: absolute;
  left: 0;
  top: 80px;
  background-color: #00ade1;
}
.link-select #tab li {
  height: 25px;
  line-height: 25px;
  display: block;
  float: left;
  text-align: center;
  padding: 0 10px;
  border: 1px solid #00ade1;
  box-sizing: border-box;
  background-color: #fff;
  color: #00ade1;
}
.link-select #tab .currentTab {
  background-color: #00ade1;
  color: #fff;
  border: 1px solid #fff;
  box-sizing: border-box;
}
