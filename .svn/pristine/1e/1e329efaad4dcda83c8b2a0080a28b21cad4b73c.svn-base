/*公共头部开始*/
header {
  width: 100%;
  min-width: 1199px;
  height: 80px;
  overflow: hidden;
}
header .left_logo {
  width: 20%;
  height: 100%;
  background-color: #0488d1;
  padding-top: 15px;
}
header .left_logo img {
  max-width: 66%;
  margin-left: 50px;
  margin-top: 4px;
}
header .right_tools {
  width: 80%;
  height: 100%;
}
header .right_tools nav {
  width: 75%;
  height: 100%;
  background-color: #0488d1;
  float: left;
}
header .right_tools nav #nav {
  display: flex;
  justify-content: space-around;
}
header .right_tools nav #nav li {
  color: #fff;
  width: 100%;
  height: 80px;
  line-height: 33px;
  border-left: 1px solid #1598e1;
  border-right: 1px solid #0472ae;
  background-color: #0488d1;
  box-sizing: border-box;
  text-align: center;
}
header .right_tools nav #nav li img {
  height: 25px;
  display: inline-block;
  padding-top: 18px;
}
header .right_tools nav #nav li span {
  display: block;
}
header .right_tools .user-info {
  width: 25%;
  height: 100%;
  background-color: #0488d1;
  float: left;
}
header .right_tools .user-info .weather_msg {
  width: 60%;
  height: 80px;
}
header .right_tools .user-info .weather_msg .wea_left {
  width: 40%;
  height: 100%;
  text-align: right;
  line-height: 80px;
  font-size: 15px;
}
header .right_tools .user-info .weather_msg #wea_right {
  width: 60%;
  height: 100%;
  display: inline-block;
  position: relative;
}
header .right_tools .user-info .weather_msg #wea_right img {
  position: absolute;
  width: 30px;
  left: 8px;
  top: 13px;
}
header .right_tools .user-info .weather_msg #wea_right .wea_info {
  position: absolute;
  left: 52px;
  top: 24px;
}
header .right_tools .user-info .weather_msg #wea_right .w_date {
  position: absolute;
  left: 5px;
  top: 43px;
}
header .right_tools .user-info .login_user {
  width: 40%;
  height: 40px;
  font-size: 12px;
  margin-top: 25px;
  white-space: nowrap;
}
header .right_tools .user-info .login_user a {
  width: 25%;
  float: right;
  color: #fff;
  text-decoration: none;
  padding-right: 22%;
}
header .right_tools .user-info .login_user a:nth-child(1) {
  padding-left: 5%;
}
header .right_tools .user-info .login_user a:nth-child(3) {
  padding-top: 3%;
  padding-left: 5%;
}
header .right_tools .user-info .login_user a:nth-child(4) {
  padding-top: 3%;
}
/*公共头部结束*/
/*选项卡*/
.link-select {
  width: 100%;
  min-width: 1199px;
  height: 25px;
  position: absolute;
  left: 0;
  top: 80px;
  border-bottom: 1px solid #dcdcdc;
  box-sizing: border-box;
}
.link-select #tab li {
  height: 25px;
  line-height: 25px;
  display: block;
  float: right;
  text-align: center;
  padding: 0 13px;
  border: 1px solid #00ade1;
  box-sizing: border-box;
  background-color: #0488d1;
  color: #fff;
  position: relative;
  margin-left: 2px;
  border-radius: 3px;
}
.link-select #tab li i {
  position: absolute;
  left: 65px;
  top: -6px;
}
.link-select #tab .currentTab {
  background-color: #f3f2f1;
  color: #0488d1;
  border: 1px solid #dcdcdc;
  box-sizing: border-box;
}
