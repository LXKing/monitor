<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rayton.gps.dao.icon.IconDao">


    <select id="listByType" resultType="com.rayton.gps.dao.baseinfo.MarkerFileInfo">
        SELECT *
        FROM
            icon
        WHERE type = #{type} AND companyId = #{companyId}
    </select>

    <select id="get" resultType="com.rayton.gps.dao.baseinfo.MarkerFileInfo">
        SELECT *
        FROM
            icon
        WHERE name = #{name}
    </select>
    <update id="update" parameterType="com.rayton.gps.dao.baseinfo.MarkerFileInfo">


    </update>

    <delete id="delete">


        DELETE FROM icon
        WHERE name = #{name}
    </delete>

    <insert id="insert" parameterType="com.rayton.gps.dao.baseinfo.MarkerFileInfo">

        INSERT INTO icon (id, name, type, time, alt, lng, companyId, showName, share) VALUES
            (#{id}, #{name}, #{type}, now(), #{alt}, #{lng}, #{companyId}, #{showName}, #{share})

    </insert>
</mapper>
