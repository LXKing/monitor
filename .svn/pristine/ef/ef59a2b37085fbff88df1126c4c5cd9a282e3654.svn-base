package com.rayton.gps.domain.permission;

/**
 * 权限信息
 *
 * @author 生
 */
public class CollectPermissionInfo {
    private String name;
    private String decription;
    private boolean allowAnoumous;

    /**
     * 获取权限名
     */
    public String getName() {
        return name;
    }

    /**
     * 设置权限名
     */
    public void setName(String name) {
        this.name = name;
    }

    /**
     * 获取描述
     */
    public String getDecription() {
        return decription;
    }

    /**
     * 设置描述
     */
    public void setDecription(String decription) {
        this.decription = decription;
    }

    /**
     * 获取是否允许匿名访问
     */
    public boolean isAllowAnoumous() {
        return allowAnoumous;
    }

    /**
     * 设置是否允许匿名访问
     */
    public void setAllowAnoumous(boolean allowAnoumous) {
        this.allowAnoumous = allowAnoumous;
    }
}
