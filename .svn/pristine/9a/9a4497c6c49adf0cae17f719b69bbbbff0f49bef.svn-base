<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>报表管理</title>


    <link th:href="@{/static/dc_file/css/layui.css}" rel="stylesheet" type="text/css"/>
    <link rel="stylesheet" th:href="@{/static/dc_file/css/report.css}">

    <!-- 引入bootstrap样式 -->
    <link href="https://cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <!-- 引入bootstrap-table样式 -->
    <link href="https://cdn.bootcss.com/bootstrap-table/1.11.1/bootstrap-table.min.css" rel="stylesheet">


    <style>

        ::-webkit-scrollbar {
            width: 14px;
            height: 8px;
            background-color: transparent;
        }

        ::-webkit-scrollbar-track {
            background-color: transparent;
        }

        ::-webkit-scrollbar-thumb {
            background-color: rgba(0, 0, 0, .1);
        }

        .layui-laypage .layui-laypage-curr .layui-laypage-em {
            background-color: #1E9FFF !important;
        }

        .layui-nav-tree .layui-nav-item a:hover {
            /*background-color: rgba(3, 107, 164,0.5);*/
        }

        .layui-nav-child dd a {
            color: #666 !important;
            background-color: #f1f1f1 !important;
        }

        .layui-nav-child {
            padding: 0;
        }


    </style>


    <style>
        * {
            border: none;
        }


    </style>
</head>

<body class="layui-layout-body" style="font-size:12px ">
<div class="layui-layout layui-layout-admin">


    <div class="layui-header  " style="    background-color:rgb(3, 107, 164);">
        <div class="layui-logo" style="color: whitesmoke">报表管理</div>


        <ul class="layui-nav layui-layout-right">
            <li class="layui-nav-item">
                <a href="javascript:;" th:text="${name}">
                    <img class="layui-nav-img"
                         src="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1524830996589&di=72721513f19e85075325358a5a7e8fd0&imgtype=0&src=http%3A%2F%2Fimg.zcool.cn%2Fcommunity%2F015f8c565565b76ac7251c94f22968.png">

                </a>
                <dl class="layui-nav-child">
                    <dd><a href="">基本资料</a></dd>

                </dl>
            </li>
            <li class="layui-nav-item"><a href="/logout">退出</a></li>
        </ul>
    </div>

    <div class="layui-side">
        <div id="#basic_header"
             style="width:200px;height:45px;line-height: 45px;background-color: #028ad6;color: #fff;padding-left: 15px;overflow-x: hidden;position: absolute;z-index: 999">
            报表管理
        </div>
        <div class="layui-side-scroll" id="view">
            <!-- 左侧导航区域（可配合layui已有的垂直导航） -->
        </div>
    </div>
    <div class="layui-body">
        <h1 class="site-h1" style="padding: 15px;"><i class="layui-icon"></i><span
                id="tableTitle"></span></h1>


        <div id="shitBar" style="padding: 15px;"></div>

        <div class="layui-progress  " lay-filter="demo" lay-showPercent="true" id="pb" style="      background-color: #fff;  ">

        </div>

        <div class="table-responsive">
            <table id="theTable"  class="table text-nowrap">
                <div id="a"></div>

            </table>
        </div>
    </div>


</div>


</body>
<script th:src="@{/static/lib/layui/layui.all.js}"></script>
<!-- jquery -->
<script src="https://cdn.bootcss.com/jquery/2.2.3/jquery.min.js"></script>
<script src="https://cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

<!-- bootstrap-table.min.js -->
<script src="https://cdn.bootcss.com/bootstrap-table/1.11.1/bootstrap-table.min.js"></script>
<!-- 引入中文语言包 -->
<script src="https://cdn.bootcss.com/bootstrap-table/1.11.1/locale/bootstrap-table-zh-CN.min.js"></script>
<script th:src="@{/static/lib/template-web.js}"></script>
<script th:src="@{/static/js/center/dateutils.js}"></script>
<script th:src="@{/static/gpsparser.js}"></script>
<script th:src="@{/static/js/center/common.js}"></script>
<!--基礎信息-->
<script src="http://api.map.baidu.com/api?v=2.0&ak=Y2TqMdQvIRuGGW83xP2kd8LR7Y5SAplh"></script>
<script th:src="@{/static/webmap.js}"></script>
<script th:src="@{/static/js/center/dateutils.js}"></script>
<script th:src="@{/static/report/rep.js}"></script>
<script th:src="@{/static/report/reportshit.js}"></script>
<script th:src="@{/static/report/renderTrip.js}"></script>
<script th:src="@{/static/report/renderAcc.js}"></script>
<script th:src="@{/static/report/renderParking.js}"></script>
<script th:src="@{/static/report/renderOverSpeed.js}"></script>

<!--左側的屎-->
<script id="nav2" type="text/html">
    <ul class="layui-nav layui-nav-tree" lay-filter="test"
        style="background-color: rgba(3, 107, 164,0.5);">
        {{each data as value }} {{ if value.pid==null }}
        <li class="layui-nav-item">
            <a class="cccc" style="display: none">{{ value.text }}

                <span class="layui-nav-more"></span>

            </a> {{set temp = value.id}}
            <dl class="layui-nav-child" style="display: block">
                {{each data as val}} {{ if val.pid== temp }}
                <dd style=" text-indent:50px;"><a href="javascript:;"
                                                  onclick="nav_to('{{val.url}}','{{ val.text }}');console.log($(this));$(this).addClass('current_a').parent().siblings('dd').children('a:nth-child(1)').removeClass('current_a');$(this).parent().parent().parent().siblings('li').children('dl').children('dd').children('a:nth-child(1)').removeClass('current_a');"
                                                  tat="{{ val.text }}">{{ val.text }}</a></dd>
                {{/if}} {{/each}}
            </dl>
        </li>
        {{/if}} {{/each}}
    </ul>
</script>


<script>
    $(function () {
        $(".layui-nav-child dd>a").mouseover(function () {
            $(this).css({"backgroundColor": "#d9e6ee"});
        })
    })
</script>
<script>

    if (window.top !== window.self) {
        jQuery(".layui-header").hide();
        jQuery(".layui-side").css("top", 0);
        jQuery(".layui-body").css("top", 0);
        // jQuery(".mmppmm").remove();

        // jQuery("button").attr("disabled", true);

        console.log("mmmp")
    }

</script>
</html>