#box_drag {
  width: 0.1%;
  height: 100%;
  position: absolute;
  left: 20%;
  top: 0;
  background-color: #fff;
  z-index: 1000;
}

#box_drag::before {
  content: '';
  background: url("./../image/zuoyou.png") no-repeat;
  background-size: contain;
  display: inline-block;
  width: 7px;
  height: 60px;
  position: absolute;
  left: -4px;
  top: 43%;
}

