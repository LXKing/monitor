/*ztree表格*/
.ztree {
    padding: 0;
    width: 90%;
}

.ztree li a {
    vertical-align: middle;
    height: 30px;
}

.ztree li > a {
    width: 100%;
}

.ztree li > a,
.ztree li a.curSelectedNode {
    padding-top: 0px;
    background: none;
    height: auto;
    border: none;
    cursor: pointer;
    opacity: 1;
    text-decoration: none !important;
}

.ztree li ul {
    /*  padding-left: 0px */
}

.ztree div.diy span {
    line-height: 30px;
    vertical-align: middle;
}

.ztree div.diy {
    height: 100%;
    width: 27%;
    line-height: 30px;
    /*  border-left: 1px solid #eeeeee; */
    text-align: center;
    display: inline-block;
    box-sizing: border-box;
    color: #6c6c6c;
    font-family: "SimSun";
    font-size: 12px;
    /*  overflow: hidden; */
    color: #208DEE;
}

.ztree div.diy:first-child {
    text-align: left;
    /*  text-indent: 10px; */
    border-left: none;
}

